//PREP parameters, return, example, pseudo code
//P: str
//R: console
//P: 
function doubleChar(str) {
  // declare a empty string  
  var word = '';
  // loop through the word
  for (var i = 0; i < str.length; i++){
    // add string together
    word = word + str[i] + str[i];
  };
  return word;
};gth
}
//E:
" String"      -> "SSttrriinngg"
"Hello World" -> "HHeelllloo  WWoorrlldd"
"1234!_ "     -> "11223344!!__  "