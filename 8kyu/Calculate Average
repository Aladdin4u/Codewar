//PREP parameters, return, example, pseudo code
//P: array
//R: console
//P: 
function find_average(array) {
  // if array is empty return 0 else average 
  return !array || array.length === 0 ?0
  :array.reduce((a,b) => a+b,0)/array.length
}
//E:
find_average([])// 0
find_average([1,1,1]) // 1
find_average([1,2,3]) // 2