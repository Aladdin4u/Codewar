//PREP parameters, return, example, pseudo code
//P: array of whole number
//R: console
//P: 
function solution(string) {
    let result = '';
  for (let i = 0; i < string.length; i += 1) {
    string[i].charCodeAt() > 96 ? result += string[i] : result += ` ${string[i]}`
  }
  return result;
}
//E:
console.log(sortArray([1,7,3,5,9,4])) // [1,3,5,7,9,4]
